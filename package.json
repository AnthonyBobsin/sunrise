{
  "name": "sunrise",
  "version": "0.0.1-beta",
  "description": "sunrise",
  "main": "lib/main.js",
  "license": "MIT",
  "engines": {
    "node": ">=4.5.0",
    "yarn": ">=0.20.3",
    "npm": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bsheikh/sunrise"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf lib",
    "compile": "node build/scripts/compile",
    "start": "nodemon build/scripts/dev-server --ignore dist --ignore coverage --ignore src",
    "start:dev": "better-npm-run start:dev",
    "start:quality": "better-npm-run start:quality",
    "start:sandbox": "better-npm-run start:sandbox",
    "start:prod": "better-npm-run start:prod",
    "deploy": "npm run lint:style && npm run lint:code && npm run test && npm run clean && npm run compile",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:quality": "better-npm-run deploy:quality",
    "deploy:sandbox": "better-npm-run deploy:sandbox",
    "deploy:prod": "better-npm-run deploy:prod",
    "lint:style": "stylelint \"src/**/*.{scss,css}\" --syntax scss",
    "lint:code": "eslint src",
    "lint-fix:code": "eslint --fix src",
    "test": "better-npm-run test",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "lint:code",
    "src/**/*.{scss,css}": "lint:style"
  },
  "betterScripts": {
    "dev": {
      "command": "npm run start",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:dev": {
      "command": "npm run start",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:sandbox": {
      "command": "npm run start",
      "env": {
        "NODE_ENV": "sandbox",
        "DEBUG": "app:*"
      }
    },
    "start:quality": {
      "command": "npm run start",
      "env": {
        "NODE_ENV": "quality",
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "npm run start",
      "env": {
        "NODE_ENV": "dev_production",
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:sandbox": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "sandbox",
        "DEBUG": "app:*"
      }
    },
    "deploy:quality": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "quality",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "jest": {
    "setupFiles": [
      "./build/config/test/test.config.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(scss|jpg|png|svg)$": "<rootDir>/build/config/test/file.mock.js"
    },
    "modulePathIgnorePatterns": [
      "/dist/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "node"
    ],
    "transform": {
      "\\.js$": "./node_modules/babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "dependencies": {
    "antd": "^2.13.7",
    "axios": "^0.17.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.13",
    "compression": "^1.6.2",
    "css-loader": "^0.26.0",
    "cssnano": "^3.7.4",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "intl": "^1.2.5",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.3",
    "normalize.css": "^5.0.0",
    "normalizr": "^3.2.3",
    "postcss-loader": "^1.1.0",
    "prop-types": "^15.5.8",
    "react": "^16.0.0",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.0.2",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-react-intl": "^2.3.1",
    "debug": "^3.1.0",
    "enzyme": "^3.1.0",
    "enzyme-react-intl": "^1.4.5",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.14.0",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "nodemon": "^1.10.2",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.0.0",
    "react-test-renderer": "^16.0.0",
    "redbox-react": "^1.2.10",
    "redux-saga-testing": "^1.0.5",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
